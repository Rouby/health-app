{
  "name": "health-app",
  "version": "1.26.3",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "dev": "nx run-many --target=dev",
    "build": "nx run-many --target=build",
    "release": "standard-version",
    "deploy": "nx run-many --target=deploy",
    "i18n": "nx run-many --target=i18n:extract",
    "prisma:push": "yarn workspace @rouby/health-app-backend pnpify prisma db push",
    "prisma:studio": "yarn workspace @rouby/health-app-backend pnpify prisma studio"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "nx": "15.2.4",
    "standard-version": "9.5.0",
    "typescript": "4.9.3"
  }
}
